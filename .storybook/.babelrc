{
  "comments": "You need to keep this in sync with shared/babel-preset",
  "presets": [
    [
      "env",
      {
	"loose": true,
	"modules": false
      }
    ],
    "react"
  ],
  "plugins": [
    "transform-class-properties",
    "transform-object-rest-spread",
    "dev-expression",
    [
      "transform-react-remove-prop-types",
      {
	"mode": "unsafe-wrap"
      }
    ],
    "transform-inline-environment-variables"
  ]
}
